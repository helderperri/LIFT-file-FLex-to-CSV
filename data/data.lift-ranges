<range-element id="5.3.7 Wear clothing" guid="4445cccd-e9b9-4f25-9e8c-2ef58408297d" parent="5.3 Clothing">
<label>
<form lang="en"><text>Wear clothing</text></form>
<form lang="es"><text>Usar prendas de vestir</text></form>
</label>
<abbrev>
<form lang="en"><text>5.3.7</text></form>
</abbrev>
