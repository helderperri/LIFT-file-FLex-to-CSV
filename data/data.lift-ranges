<form lang="pt"><text>ANA.DEM</text></form>
</abbrev>
<trait name="catalog-source-id" value=""/>
</range-element>
<range-element id="Demonstrativo catafórico" guid="b94b4b15-6a99-4497-80a8-9dff65108929" parent="Demonstrativo">
