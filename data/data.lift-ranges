<range-element id="2.2.7 Urinate, urine" guid="e7f94aea-ba50-481d-b640-d5cd8bdedc72" parent="2.2 Body functions">
<label>
<form lang="en"><text>Urinate, urine</text></form>
<form lang="es"><text>Orinar, orina</text></form>
</label>
<abbrev>
<form lang="en"><text>2.2.7</text></form>
</abbrev>
<description>
