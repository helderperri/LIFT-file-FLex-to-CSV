<form lang="pt"><text>Demonstrativo catafórico</text></form>
</label>
<abbrev>
<form lang="en"><text>CAT.DEM</text></form>
<form lang="pt"><text>CAT.DEM</text></form>
</abbrev>
<trait name="catalog-source-id" value=""/>
